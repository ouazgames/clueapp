{
  "appId": "com.ouazgames.cluesgames.cluegame",
  "appName": "Clue Game",
  "webDir": "www",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#1a1a2e"
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "AdMob": {
      "appIdAndroid": "ca-app-pub-3940256099942544~3347511713",
      "appIdIos": "ca-app-pub-9762641206922107~4187210251"
    }
  },
  "android": {
    "allowMixedContent": true
  },
  "ios": {
    "contentInset": "automatic",
    "preferredContentMode": "mobile",
    "scrollEnabled": true,
    "limitsNavigationsToAppBoundDomains": false
  }
}